{
  "name": "osb-controller",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "npm-run-all -p dev:*",
    "dev:remix": "remix vite:dev --host",
    "dev:worker": "env-cmd -f .env node ./worker/worker.mjs",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "start:worker": "node ./worker/worker.mjs",
    "start:tts": "docker run -d --name=osb-tts -v ./tts:/output -p 8080:8080 openschoolbell/tts:main",
    "stop:tts": "docker stop osb-tts && docker remove osb-tts",
    "typecheck": "tsc",
    "prettier": "prettier app --check --end-of-line auto"
  },
  "dependencies": {
    "@arcath/utils": "^3.15.0",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.5.0",
    "@remix-run/node": "^2.16.4",
    "@remix-run/react": "^2.16.4",
    "@remix-run/serve": "^2.16.4",
    "adm-zip": "^0.5.16",
    "bullmq": "^5.47.1",
    "date-fns": "^4.1.0",
    "isbot": "^5.1.25",
    "jsonwebtoken": "^9.0.2",
    "mkdirp": "^3.0.1",
    "prisma": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.4",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/adm-zip": "^0.5.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "autoprefixer": "^10.4.21",
    "env-cmd": "^10.1.0",
    "eslint": "^9.24.0",
    "eslint-import-resolver-typescript": "^4.3.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.3",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.29.0",
    "vite": "^6.2.5",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "prisma": {
    "seed": "node prisma/seed.js"
  }
}
